{
    "env": {
        "browser": true,
        "es5": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "generators": false,
            "objectLiteralDuplicateProperties": false
        }
    },
    "globals": {
        "$": false
    },
    "extends": [
        "plugin:prettier/recommended"
    ],
    "rules": {
        "indent": [
            2,
            2
        ],
        "quotes": [
            2,
            "single"
        ],
        "semi": [
            2,
            "always"
        ]
    }
}
